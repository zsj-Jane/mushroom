<!--pages/home/home.wxml-->
<view class="home-container">
	<!-- 轮播图 -->
	<swiper indicator-dots autoplay circular interval="{{5000}}" duration="{{1000}}">
		<block wx:for="{{swipers}}" wx:key="*this">
			<swiper-item>
        <image src="{{item.img_url}}"></image>
			</swiper-item>
		</block>
	</swiper>
</view>